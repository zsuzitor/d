




@model dip.Models.Domain.Action
@if (ViewBag.prosChild.Count != 0)
{
    <div id=@ViewBag.parent name="1">
        <table>
            <tr>
                @{
                    int cnt = 0;
                    foreach (var pros in ViewBag.prosChild)
                    {
                        if (cnt++ % 3 == 0)
                        {
                            @:</tr><tr>
                    }
                        @:<td>
                        if (pros.IsSelected == true)
                        {
                            <input type="checkbox" name="listSelectedPros" value="@pros.Id" checked /> @pros.Name
                        }
                        else
                        {
                            <input type="checkbox" name="listSelectedPros" value="@pros.Id" /> @pros.Name
                        }
                        <div id=@pros.Id name="0"></div>
                        @:</td>
                }
                    @:</tr>
                }
            </table>
        </div>
                    }
