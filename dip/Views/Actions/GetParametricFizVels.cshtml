



@model dip.Models.Domain.Action
<div class="form-group" id="parametricFizVel">
    @if (ViewBag.parametricFizVelId.Count != 0)
    {
        @Html.Label("Название физической величины", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("parametricFizVelId", ViewBag.parametricFizVelId as SelectList, htmlAttributes: new { @class = "form-control" })
        </div>
    }
</div>