@model dip.Models.Domain.FEText

<dl class="dl-horizontal">
    @Html.HiddenFor(x1 => x1.IDFE)
    <dt>
        @Html.DisplayName("Название")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.Name,new { @class = "form-control" })
    </dd>
    <dt>
        @Html.DisplayName("Описание")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.Text, new { @class = "form-control" })
    </dd>

    <dt>
        @Html.DisplayName("Входное воздействие")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.TextInp, new { @class = "form-control" })
    </dd>

    <dt>
        @Html.DisplayName("Выходное воздействие")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.TextOut, new { @class = "form-control" })
    </dd>

    <dt>
        @Html.DisplayName("Объект")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.TextObj, new { @class = "form-control" })
    </dd>

    <dt>
        @Html.DisplayName("Применение")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.TextApp, new { @class = "form-control" })
    </dd>

    <dt>
        @Html.DisplayName("Литература")
    </dt>

    <dd>
        @Html.TextAreaFor(x1 => x1.TextLit, new { @class = "form-control" })
    </dd>

</dl>