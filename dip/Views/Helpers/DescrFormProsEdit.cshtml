

@model dip.Models.ViewModel.HelpersV.DescrFormListDataV<dip.Models.Domain.Pro>



<div class="form-group descrCheckBoxChild" @*id=@("pros" + type)*@>
    <div class="col-md-10" id="@(Model.ParentId+"_PROS")">
        @if (Model.List != null)
        {

            foreach (var pro in Model.List)
            {
                <div>
                    <label>
                        <input @*id=@("listSelectedPros"+type+cnt)*@ class="checkBoxClass" type="checkbox" name=@("listSelectedPros") value="@pro.Id" />

                        @pro.Name
                    </label>

                    <div id="@pro.Id">

                    </div>

                </div>
            }


        }
    </div>
    <div class="col-md-10">
        <button  onclick="addNewPro('@Model.ParentId')">Добавить</button>
    </div>
</div>

