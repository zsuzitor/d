@model IEnumerable<dip.Models.TechnicalFunctions.OperandGroup>

@{
    ViewBag.Title = "Группы объектов технических функций";
}

<h2>Группы объектов технических функций</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Дескриптор")
        </th>
        <th>
            @Html.DisplayName("Значение")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(x => item.Id)
            </td>
            <td>
                @Html.DisplayFor(x => item.Value)
            </td>
        </tr>
    }

</table>
