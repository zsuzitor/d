@model IEnumerable<dip.Models.TechnicalFunctions.Index>

@{
    ViewBag.Title = "Проиндексированные технические функции";
}

<h2>Проиндексированные технические функции</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Дескриптор")
        </th>
        <th>
            @Html.DisplayName("Дескриптор действия")
        </th>
        <th>
            @Html.DisplayName("Дескриптор объекта")
        </th>
        <th>
            @Html.DisplayName("Дескриптор ограничения")
        </th>
        <th>
            @Html.DisplayName("Идентификаторы физических эффектов")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
                
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Operation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Operand.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Limit.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EffectIds)
            </td>
        </tr>
    }

</table>
