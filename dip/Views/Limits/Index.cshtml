@model IEnumerable<dip.Models.TechnicalFunctions.Limit>

@{
    ViewBag.Title = "Ограничения технических функций";
}

<h2>Ограничения технических функций</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Дескриптор")
        </th>
        <th>
            @Html.DisplayName("Значение")
        </th>
        <th>
            @Html.DisplayName("Дескриптор родителя")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(x => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parent)
            </td>
        </tr>
    }

</table>
